{
    "manifest_version": 2,
    "name": "YouControl",
    "description": "The best youtube controller ever.",
    "version": "1",
    "author": "Boris Nguimmo",
    "browser_action": {
      "default_icon": "youtube.png",
      "default_title": "Have a good day",
      "default_popup": "popup.html"
    },
    "permissions": ["activeTab","tabs", "<all_urls>"]
   }